.forms
  width: 100%
  height: 1150px
.table-borders
  width: 970px
  margin: 50px auto
table
  border-collapse: collapse
  border: solid #ccc
  border-width: 1px 0px 0px 1px
  background-color: white
th,td
  border: solid #ccc
  border-width: 0px 1px 1px 0px
  padding: .75rem
thead
  background-color: #62d89b
  font-size: 18px
  color: #fff
  text-align: center
tbody th
  color: #373a3c
.module-text  
  text-align: center
  color: #373a3c
.funtion-text
  line-height: 2.5
  color: #373a3c 
.hidden-xl-down
  display: none
.check-result
  color: #373a3c
  font-size: 20px
  text-align: center
.check-count
  color: #de0357
  font-size: 30px
.web-b
  float: left
  width: 40%
  height: 100px
  margin: 50px 5%
.web-butn1
  cursor: pointer
  outline: none
  position: relative
  left: 35%
  width: 60%
  height: 35px
  line-height: 20px
  font-size: 1em
  background-color: #ecedf1
  border-radius: 2rem
  border: 2px solid #de0357
  color: #de0357
.web-butn2
  cursor: pointer
  outline: none
  position: relative
  left: 15%
  width: 60%
  height: 35px
  line-height: 20px
  font-size: 1em
  background-color: #ecedf1
  border-radius: 2rem
  border: 2px solid #62d89b
  color: #62d89b;
  
.web-butn1:hover
  background-color:  #de0357
  color: #ecedf1
  transition: all .5s linear
  box-shadow: 5px 5px 10px #373a3c
.web-butn2:hover
  background-color:  #62d89b
  color: #ecedf1
  transition: all .5s linear
  box-shadow: 5px 5px 10px #373a3c
  

